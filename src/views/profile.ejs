    <main>
        <div class="container">
            <div class="center">
                <h1>Perfil</h1>
                <p class="blue-text text-darken-2">
                    ID: <%= user.id %><br />
                    Email: <%= user.email%><br />
                    Tipo: <%= user.tipo%> <% if (user.tipo === '1') { %>
                    (Administrador)
                    <%}%><br />
                    </p>
                        <img src=" <%= typeof file != 'undefined' ? file : '' %>" class="materialboxed responsive-img"
                    >
                    <p>
                        QR:
                    </p>
                    <img src="<%= typeof qr != 'undefined' ? qr : '' %>" class="materialboxed responsive-img">
            </div>
        </div>
    </main>
</body>

</html>