<!DOCTYPE html>
<html lang="en">

<script src="../public/sidenav.js"></script>
<!-- <script src="https://code.jquery.com/jquery-3.5.1.min.js"
    integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script> -->
<script type="text/javascript" src="https://code.jquery.com/jquery-2.2.1.min.js"></script>
<script type="text/javascript"
    src="https://cdn.rawgit.com/pinzon1992/materialize_table_pagination/f9a8478f/js/pagination.js"></script>
<script src="../public/js/table.js"></script>
<!-- <link rel="stylesheet" href="../public/css/sidenav.css"> -->
<script src="https://printjs-4de6.kxcdn.com/print.min.js"></script>
<link rel="stylesheet" href="https://printjs-4de6.kxcdn.com/print.min.css">
<title>Home</title>

<body>
    <%- include home.ejs %>
    <main>
        <div class="container">
            <div class="row">
                <form action="/test2" method="POST">
                    <table class="highlight centered" id="tb_asegurados">
                        <thead>
                            <tr>
                                <th>CODIGO</th>
                                <th>NOMBRE</th>
                                <th>FEC. NAC.</th>
                                <th>SEXO</th>
                                <th>CI.</th>
                                <th>TIPO DE SANGRE</th>
                                <th>TEST</th>
                            </tr>
                        </thead>

                        <tbody>
                            <% for(var i=0; i<asegurados.length; i++) {%>
                            <tr>
                                <td><%= asegurados[i].cod %></td>
                                <td><%= asegurados[i].name %></td>
                                <td><%= asegurados[i].fec_nac %></td>
                                <td><%= asegurados[i].sexo %></td>
                                <td><%= asegurados[i].ci %> <%= asegurados[i].ci_loc %></td>
                                <td><%= asegurados[i].tipo_sangre %></td>

                                <td><button class="btn" name="cod_ase" type="submit" onclick="printJS({ printable: 'tb_asegurados', type: 'html', css:[
                                        'https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.css']
                                        })" value="<%= asegurados[i].cod %>">TEST</button></td>
                            </tr>
                            <% } %>
                        </tbody>
                    </table>
                </form>
            </div>
        </div>
        <div class="col-md-12 center text-center">
            <span class="left" id="total_reg"></span>
            <ul class="pagination pager" id="myPager"></ul>
        </div>
    </main>
</body>

</html>